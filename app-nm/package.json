{
  "name": "app-nm",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test:client": "ng test",
    "test:headless": "ng test --watch=false --browsers=ChromeHeadless",
    "test:lint:e2e": "ng lint && npm run test:headless && ng e2e --protractor-config='e2e/protractor.ci.conf.js'",
    "build": "ng build",
    "start": "node dist/server",
    "client:dev": "ng serve --proxy-config proxy.config.json",
    "server:dev": "nodemon server.ts",
    "build:client": "ng build --aot --prod --build-optimizer",
    "build:server": "webpack --config webpack.server.config.js --progress --colors",
    "build:ssr": "npm run build:client-and-ssr-bundles && npm run build:server",
    "build:client-and-ssr-bundles": "npm run build:client && ng run app-nm:server:production --bundleDependencies all",
    "postinstall": "npm run build:ssr"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^0.801.0",
    "@angular/animations": "^8.2.0-next.0",
    "@angular/cdk": "^8.0.2",
    "@angular/cli": "^8.1.0",
    "@angular/common": "^8.2.0-next.0",
    "@angular/compiler": "^8.2.0-next.0",
    "@angular/compiler-cli": "^8.2.0-next.0",
    "@angular/core": "^8.2.0-next.0",
    "@angular/forms": "^8.2.0-next.0",
    "@angular/material": "^8.0.2",
    "@angular/platform-browser": "^8.2.0-next.0",
    "@angular/platform-browser-dynamic": "^8.2.0-next.0",
    "@angular/platform-server": "^8.2.0-next.0",
    "@angular/pwa": "^0.800.4",
    "@angular/router": "^8.2.0-next.0",
    "@angular/service-worker": "^8.2.0-next.0",
    "@ngrx/effects": "^8.0.1",
    "@ngrx/router-store": "^8.0.1",
    "@ngrx/store": "^8.0.0",
    "@ngrx/store-devtools": "^8.0.1",
    "@nguniversal/express-engine": "8.1.1",
    "@nguniversal/module-map-ngfactory-loader": "8.1.1",
    "@sendgrid/mail": "^6.4.0",
    "@types/express": "^4.17.0",
    "angular-svg-icon": "^7.2.1",
    "body-parser": "^1.19.0",
    "core-js": "^2.5.4",
    "express": "^4.17.1",
    "foundation-sites": "^6.5.3",
    "hammerjs": "^2.0.8",
    "ngx-page-scroll": "^6.0.2",
    "ngx-page-scroll-core": "^6.0.2",
    "rxjs": "~6.5.2",
    "tslib": "^1.10.0",
    "typescript": "~3.4.5",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular/language-service": "^8.2.0-next.0",
    "@ngrx/schematics": "^8.0.1",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "^1.19.1",
    "protractor": "~5.4.0",
    "ts-loader": "^6.0.4",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "webpack-cli": "^3.3.5"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      "node_modules/**/node_modules"
    ],
    "delay": "2500",
    "env": {
      "NODE_ENV": "development",
      "NODE_CONFIG_DIR": "./config"
    }
  }
}
