    "@angular/forms": "^9.0.6",
    "@angular/material": "^9.1.2",
    "@angular/platform-browser": "^9.0.6",
    "@angular/platform-browser-dynamic": "^9.0.6",
    "@angular/platform-server": "^9.0.6",
    "@angular/router": "^9.0.6",
    "@angular/service-worker": "^9.0.6",
    "@ngrx/effects": "^9.0.0",
    "@ngrx/router-store": "^9.0.0",
    "@ngrx/store": "^9.0.0",
    "@ngrx/store-devtools": "^9.0.0",
    "@nguniversal/common": "^9.0.2",
    "@nguniversal/express-engine": "^9.0.2",
    "@sendgrid/mail": "^6.5.4",
    "body-parser": "^1.19.0",
    "core-js": "^3.6.4",
    "express": "^4.17.1",
    "hammerjs": "^2.0.8",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "ngx-page-scroll": "^7.0.1",
    "ngx-page-scroll-core": "^7.0.1",
    "rxjs": "~6.5.4",
    "tslib": "^1.11.1",
    "zone.js": "~0.10.2"
