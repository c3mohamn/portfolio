{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "Online portfolio for Nasir Mohammad",
  "author": "Nasir Mohammad",
  "scripts": {
    "ng": "ng",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start": "node dist/server",
    "build": "ng build",
    "test:ci": "npm run test:headless",
    "test:client": "ng test",
    "test:headless": "ng test --watch=false --browsers=ChromeHeadless",
    "test:lint:e2e": "ng lint && npm run test:headless && ng e2e --protractor-config='e2e/protractor.ci.conf.js'",
    "test:ssr": "npm run build:ssr && npm run build:server:dev && npm start",
    "client:dev": "ng serve --proxy-config proxy.config.json",
    "server:dev": "npm run build:ssr && nodemon server.ts",
    "server:dev:no-build": "nodemon server.ts",
    "all:dev": "concurrently \"npm:client:dev\" \"npm:server:dev\"",
    "build:client": "ng build --aot --prod --build-optimizer",
    "build:server": "webpack --mode production --config webpack.server.config.js --progress --colors",
    "build:server:dev": "webpack --mode development --config webpack.server.config.js --progress --colors",
    "build:ssr": "npm run build:client-and-ssr-bundles",
    "build:client-and-ssr-bundles": "npm run build:client && ng run portfolio:server:production --bundleDependencies all",
    "prettier:format": "prettier --print-width 80 --single-quote --trailing-comma none --write **/*.ts",
    "heroku-postbuild": "npm run build:ssr && npm run build:server"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.1.1",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/material": "^8.1.1",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/platform-server": "^8.2.14",
    "@angular/pwa": "^0.800.6",
    "@angular/router": "^8.2.14",
    "@angular/service-worker": "^8.2.14",
    "@ngrx/effects": "^8.1.0",
    "@ngrx/router-store": "^8.1.0",
    "@ngrx/store": "^8.1.0",
    "@ngrx/store-devtools": "^8.1.0",
    "@nguniversal/express-engine": "8.1.1",
    "@nguniversal/module-map-ngfactory-loader": "8.1.1",
    "@sendgrid/mail": "^6.4.0",
    "body-parser": "^1.19.0",
    "core-js": "^2.5.4",
    "express": "^4.17.1",
    "hammerjs": "^2.0.8",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.2.7",
    "mongoose": "^5.6.7",
    "ngx-page-scroll": "^6.0.2",
    "ngx-page-scroll-core": "^6.0.2",
    "rxjs": "~6.5.4",
    "swagger-ui-express": "^4.0.7",
    "tslib": "^1.10.0",
    "typegoose": "^5.8.1",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.25",
    "@angular/cli": "^8.3.25",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@ngrx/schematics": "^8.1.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/joi": "^14.3.3",
    "@types/mongoose": "^5.5.11",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.1",
    "concurrently": "^4.1.1",
    "cross-env": "^5.2.0",
    "foundation-sites": "^6.5.3",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.6",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "^1.19.1",
    "protractor": "~5.4.0",
    "ts-loader": "^6.0.4",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.5.3",
    "webpack-cli": "^3.3.6"
  },
  "homepage": "https://github.com/c3mohamn/portfolio#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/c3mohamn/portfolio.git"
  },
  "bugs": {
    "url": "https://github.com/c3mohamn/portfolio/issues"
  },
  "license": "MIT",
  "engines": {
    "node": "11.x",
    "npm": "6.x"
  },
  "keywords": [
    "Nasir",
    "Mohammad",
    "Portfolio",
    "Angular",
    "App",
    "Website",
    "Mongo",
    "Db",
    "Resume",
    "Blog",
    "Express",
    "Node",
    "Typescript"
  ],
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      "node_modules/**/node_modules"
    ],
    "delay": "2500",
    "env": {
      "NODE_ENV": "development",
      "NODE_CONFIG_DIR": "./config"
    }
  }
}
