{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "Online portfolio for Nasir Mohammad",
  "author": "Nasir Mohammad",
  "scripts": {
    "ng": "ng",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start": "node dist/server",
    "build": "ng build",
    "test:ci": "npm run test:headless",
    "test:client": "ng test",
    "test:headless": "ng test --watch=false --browsers=ChromeHeadless",
    "test:lint:e2e": "ng lint && npm run test:headless && ng e2e --protractor-config='e2e/protractor.ci.conf.js'",
    "test:ssr": "npm run build:ssr && npm run build:server:dev && npm start",
    "client:dev": "ng serve --proxy-config proxy.config.json",
    "server:dev": "npm run build:ssr && nodemon server.ts",
    "server:dev:no-build": "nodemon server.ts",
    "all:dev": "concurrently \"npm:client:dev\" \"npm:server:dev\"",
    "build:client": "ng build --aot --prod --build-optimizer",
    "build:server": "webpack --mode production --config webpack.server.config.js --progress --colors",
    "build:server:dev": "webpack --mode development --config webpack.server.config.js --progress --colors",
    "build:ssr": "npm run build:client-and-ssr-bundles",
    "build:client-and-ssr-bundles": "npm run build:client && ng run portfolio:server:production",
    "prettier:format": "prettier --print-width 80 --single-quote --trailing-comma none --write **/*.ts",
    "heroku-postbuild": "npm run build:ssr && npm run build:server",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.0",
    "@angular/cdk": "^9.0.0",
    "@angular/common": "^9.0.0",
    "@angular/compiler": "^9.0.0",
    "@angular/core": "^9.0.0",
    "@angular/forms": "^9.0.0",
    "@angular/material": "^9.0.0",
    "@angular/platform-browser": "^9.0.0",
    "@angular/platform-browser-dynamic": "^9.0.0",
    "@angular/platform-server": "^9.0.0",
    "@angular/router": "^9.0.0",
    "@angular/service-worker": "^9.0.0",
    "@hasezoey/typegoose": "^5.9.2",
    "@ngrx/effects": "^8.6.0",
    "@ngrx/router-store": "^8.6.0",
    "@ngrx/store": "^8.6.0",
    "@ngrx/store-devtools": "^8.6.0",
    "@nguniversal/express-engine": "9.0.0",
    "@nguniversal/module-map-ngfactory-loader": "8.1.1",
    "@sendgrid/mail": "^6.5.2",
    "body-parser": "^1.19.0",
    "core-js": "^2.6.11",
    "express": "^4.17.1",
    "hammerjs": "^2.0.8",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.5.2",
    "mongodb-client-encryption": "^1.0.1",
    "mongoose": "^5.8.11",
    "ngx-page-scroll": "^6.0.2",
    "ngx-page-scroll-core": "^6.0.2",
    "rxjs": "~6.5.4",
    "swagger-ui-express": "^4.1.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.1",
    "@angular-devkit/schematics": "^9.0.1",
    "@angular/cli": "^9.0.1",
    "@angular/compiler-cli": "^9.0.0",
    "@angular/language-service": "^9.0.0",
    "@ngrx/schematics": "^8.6.0",
    "@schematics/angular": "^9.0.1",
    "@types/express": "^4.17.2",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "^2.0.8",
    "@types/joi": "^14.3.4",
    "@types/mongoose": "^5.7.1",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "concurrently": "^4.1.2",
    "cross-env": "^5.2.1",
    "foundation-sites": "^6.6.1",
    "husky": "^4.2.2",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "^1.19.4",
    "protractor": "^5.4.3",
    "ts-loader": "^6.2.1",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.7.5",
    "webpack-cli": "^3.3.11"
  },
  "homepage": "https://github.com/c3mohamn/portfolio#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/c3mohamn/portfolio.git"
  },
  "bugs": {
    "url": "https://github.com/c3mohamn/portfolio/issues"
  },
  "license": "MIT",
  "engines": {
    "node": "12.16.0",
    "npm": "6.13.7"
  },
  "keywords": [
    "Nasir",
    "Mohammad",
    "Portfolio",
    "Angular",
    "App",
    "Website",
    "Mongo",
    "Db",
    "Resume",
    "Blog",
    "Express",
    "Node",
    "Typescript"
  ],
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      "node_modules/**/node_modules"
    ],
    "delay": "2500",
    "env": {
      "NODE_ENV": "development",
      "NODE_CONFIG_DIR": "./config"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "ng lint",
      "pre-push": "npm run test:headless"
    }
  }
}
