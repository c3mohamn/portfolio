    "@types/jasmine": "^3.5.9",
    "@types/jasminewd2": "^2.0.8",
    "@types/joi": "^14.3.4",
    "@types/node": "^12.12.30",
    "codelyzer": "^5.1.2",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.2",
    "foundation-sites": "^6.6.1",
    "husky": "^4.2.3",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
