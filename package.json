{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "Online portfolio for Nasir Mohammad",
  "author": "Nasir Mohammad",
  "scripts": {
    "ng": "ng",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start": "node dist/server/main",
    "start:local": "npm run build:prod && npm start",
    "build": "ng build",
    "test:ci": "npm run test:headless",
    "test:client": "ng test",
    "test:headless": "ng test --watch=false --browsers=ChromeHeadless",
    "test:lint:e2e": "ng lint && npm run test:headless && ng e2e --protractor-config='e2e/protractor.ci.conf.js'",
    "client:dev": "ng serve",
    "client:prod": "ng build --prod",
    "ssr:dev": "ng run portfolio:serve-ssr",
    "ssr:prod": "ng run portfolio:server:production",
    "prerender": "ng run portfolio:prerender",
    "build:prod": "npm run client:prod && npm run ssr:prod",
    "prettier:format": "prettier --print-width 120 --single-quote --trailing-comma none --write **/*.ts",
    "heroku-postbuild": "npm run build:prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.6",
    "@angular/cdk": "^9.1.2",
    "@angular/common": "^9.0.6",
